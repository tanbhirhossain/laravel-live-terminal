{
    "name": "tanbhirhossain/laravel-live-terminal",
    "description": "A simple and secure web-based terminal to run Laravel Artisan commands.",
    "keywords": ["laravel", "terminal", "artisan", "web-terminal"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Tanbhir Hossain",
            "email": "tanbhirhossaincs@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tanbhirhossain\\LaravelLiveTerminal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanbhirhossain\\LaravelLiveTerminal\\TerminalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}